{
    "name": "fspodium-web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "lint": "bun run --bun eslint --fix .",
        "lint:styles": "bun run --bun stylelint **/*.css --fix --ignore-path .gitignore",
        "format": "bun run --bun prettier . --write --ignore-path .gitignore",
        "dev": "bun run --bun --host=0.0.0.0 --inspect=0.0.0.0 next dev --turbopack",
        "build": "bun run --bun next build",
        "start": "bun run --bun next start",
        "ui:add": "bun run --bun shadcn add",
        "prepare": "bun run --bun husky",
        "db:gen": "bun run prisma generate",
        "db:push": "bun run prisma db push",
        "db:seed": "bun src/prisma/seed.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "prisma": {
        "schema": "./src/prisma/schema.prisma"
    },
    "dependencies": {
        "next": "~15.3.2",
        "react": "~19.1.0"
    },
    "devDependencies": {
        "@auth/prisma-adapter": "^2.9.1",
        "@eslint/compat": "^1.2.9",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.27.0",
        "@fullcalendar/core": "^6.1.15",
        "@fullcalendar/daygrid": "^6.1.15",
        "@fullcalendar/interaction": "^6.1.15",
        "@fullcalendar/react": "^6.1.15",
        "@fullcalendar/timegrid": "^6.1.15",
        "@hookform/resolvers": "^5.0.1",
        "@prisma/client": "^6.8.2",
        "@radix-ui/react-accordion": "^1.2.11",
        "@radix-ui/react-avatar": "^1.1.10",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.5",
        "@radix-ui/react-tabs": "^1.1.12",
        "@tailwindcss/postcss": "^4.1.8",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/bcryptjs": "^3.0.0",
        "@types/node": "^22",
        "@types/nodemailer": "^6.4.17",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@types/uuid": "^9.0.8",
        "autoprefixer": "^10.4.21",
        "bcryptjs": "^3.0.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "eslint": "^9.27.0",
        "eslint-config-next": "~15.3.2",
        "eslint-config-prettier": "^10.1.5",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "lucide-react": "^0.511.0",
        "next-auth": "^4.24.11",
        "next-themes": "^0.4.6",
        "nodemailer": "^6.10.1",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "prisma": "^6.8.2",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.56.4",
        "react-spinners": "^0.17.0",
        "recharts": "^2.15.3",
        "shadcn": "^2.5.0",
        "sonner": "^2.0.3",
        "stylelint": "^16.19.1",
        "stylelint-config-recommended": "^16.0.0",
        "stylelint-config-tailwindcss": "^1.0.0",
        "tailwind-merge": "^3.3.0",
        "tailwindcss": "^4.1.7",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.32.1",
        "uuid": "^11.1.0",
        "vaul": "^1.1.2",
        "zod": "^3.25.32"
    }
}
